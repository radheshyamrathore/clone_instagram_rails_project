<h1>Users</h1>
<%
=begin%>
 <ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.user_name,user %>
      <%= user.user_age%>
      <%=user.email%>
    </li>
  <% end %>
</ul> 
<%
=end%>
 <%= link_to "New User", new_user_path %>

 <h1> Hello and Welcome <h1> 
<% if current_user %>
    <%= link_to 'Logout', '/logout', :data => { :confirm => 'Are you sure?' }%>
<% else %>
    <%= link_to 'Signup', '/signup'%>
    <%= link_to 'Login', '/login'%>
<% end %>
<% if flash[:notice] %>
<%= flash[:notice] %>
<% end %>